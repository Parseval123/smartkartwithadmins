<% provide(:title, 'Edit Owner') %>
<%= form_for(@owner) do |f| %>
<%= render 'shared/error_messages4' %>


   
    <%= f.label :prezzo %><br>
    <%= f.number_field :price , :step => 0.01%></br>

    <%= f.label :stock %><br>
    <%= f.number_field :stock%></br>
 

    <%= f.hidden_field :market_id, value: @owner.market_id %>

    <%= f.hidden_field :product_id, value: @owner.product_id %>	

<%= f.submit "save changes", class: "btn btn-primary" %></br>

<%end%>
